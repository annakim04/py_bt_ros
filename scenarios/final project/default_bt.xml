<?xml version="1.0" encoding="UTF-8"?>
<root main_tree_to_execute="MainTree">

  <BehaviorTree ID="MainTree">

    <Sequence>
      <MoveToCharge/>

      <!-- 1) 충전 장소 -> 택배 수령 장소 -->
      <MoveToPickup/>

      <!-- 2) 택배 수령 (항상 성공, 더미) -->
      <ReceiveParcel/>

      <!-- 3) 수령 장소 -> 배달 장소 -->
      <MoveToDelivery/>

      <!-- 4) 택배 배달 (항상 성공, 더미) -->
      <DropoffParcel/>

      <!-- 5) 배달 장소 -> 다시 수령 장소 -->
      <MoveToPickup/>

      <!-- 6) 수령 장소 -> 충전 장소 복귀 -->
      <MoveToCharge/>
    </Sequence>

  </BehaviorTree>

</root>
